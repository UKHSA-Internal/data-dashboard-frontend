TZ="Europe/London"

# NextJS Cache Revalidation
# After the amount of seconds has passed, stale page data will be refetched by NextJS.
# Disable this behaviour by setting this value to false. i.e. for debugging
NEXT_REVALIDATE_TIME=300

# Public API url
# This is the href of the "API" link in the navigation
PUBLIC_API_URL=

# Google Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID=

#######################################################################################################
# In deployed environments our Github Actions workflow injects the following variables via docker args
# For local testing against deployed APIs, please populate these values in a .env.local file. 
#
# NOTE: Don't commit these values to version control.
#######################################################################################################
API_KEY=
API_URL=

