TZ="Europe/London"

# Use local msw mocks instead of a real api
NEXT_PUBLIC_API_MOCKING=enabled

# Api key for NextJS's ISR revalidation via our internal API route
NEXT_REVALIDATE_API_KEY=

# Duration in seconds for automatic ISR cache invalidation
# Caution: this will refetch _all_ data from _every_ endpoint!
NEXT_REVALIDATE_TIME=10

# TODO: Why do we need these? Remove if possible.
NEXT_PUBLIC_BASE_URL=https://coronavirus.data.gov.uk

#######################################################################################################
# In deployed environments our Github Actions workflow injects the following variables via docker args
# The test environment is entirely stubbed using MSW.
#######################################################################################################
API_KEY='fake-api-key'
API_URL=http://fake-backend.gov.uk
